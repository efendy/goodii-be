{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "order_ref": {
      "type": "string"
    },
    "gross_amount": {
      "type": "integer"
    },
    "net_amount": {
      "type": "integer"
    },
    "tax_rate": {
      "type": "integer"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "quote",
        "pending_payment",
        "paid",
        "in_transit",
        "delivered",
        "closed",
        "cancelled"
      ]
    },
    "paid_datetime": {
      "type": "datetime"
    },
    "delivered_datetime": {
      "type": "datetime"
    },
    "listing": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::listing.listing"
    },
    "user_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-profile.user-profile"
    },
    "listing_owner_id": {
      "type": "biginteger"
    },
    "owner_id": {
      "type": "biginteger"
    }
  }
}
