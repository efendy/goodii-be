{
  "kind": "collectionType",
  "collectionName": "payment_transactions",
  "info": {
    "singularName": "payment-transaction",
    "pluralName": "payment-transactions",
    "displayName": "Payment Transaction"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "order": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::order.order",
      "inversedBy": "payment_transaction"
    },
    "external_data": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "json"
    },
    "is_verified": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "new",
        "error",
        "complete"
      ],
      "default": "new"
    }
  }
}
