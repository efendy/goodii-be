{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "enumeration",
      "enum": [
        "Food",
        "Craft",
        "Service"
      ]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "integer"
    },
    "currency": {
      "type": "string"
    },
    "shop": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shop.shop",
      "inversedBy": "listings"
    },
    "geo_hash": {
      "type": "string"
    },
    "extra_menu": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "onsale",
        "soldout",
        "hidden"
      ]
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "listing"
    }
  }
}
