{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "enumeration",
      "enum": [
        "Food",
        "Craft",
        "Service"
      ]
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "integer"
    },
    "currency": {
      "type": "string"
    },
    "geo_hash": {
      "type": "string"
    },
    "extra_menu": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "onsale",
        "soldout",
        "hidden"
      ]
    },
    "shop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shop.shop"
    },
    "listing_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::listing-tag.listing-tag",
      "inversedBy": "listings"
    }
  }
}
