{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "category": {
      "type": "enumeration",
      "enum": [
        "food",
        "craft",
        "service"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "price": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "currency": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "extra_menu": {
      "type": "json",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "onsale",
        "soldout",
        "hidden"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "shop": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shop.shop",
      "inversedBy": "listings"
    },
    "listing_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::listing-tag.listing-tag",
      "inversedBy": "listings"
    },
    "title_en": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string"
    },
    "description_en": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "richtext"
    },
    "owner_id": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger"
    },
    "uid": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "view_count": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "biginteger",
      "default": "0"
    },
    "is_fixed_price": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "short_description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "short_description_en": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "listing_add_ons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::listing-add-on.listing-add-on",
      "mappedBy": "listings"
    },
    "is_published": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "is_preorder": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "eta_minutes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer",
      "default": 0
    },
    "eta_at": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "date"
    },
    "is_test": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "boolean",
      "default": false
    },
    "geolocation": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
