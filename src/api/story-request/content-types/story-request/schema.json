{
  "kind": "collectionType",
  "collectionName": "story_requests",
  "info": {
    "singularName": "story-request",
    "pluralName": "story-requests",
    "displayName": "Story Request",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "message": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false
    },
    "shop": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::shop.shop"
    },
    "user_profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-profile.user-profile"
    },
    "message_reply": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "notes": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "status": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "new",
        "inprogress",
        "done"
      ],
      "default": "new"
    },
    "story_price_plan": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::story-price-plan.story-price-plan",
      "inversedBy": "story_request"
    }
  }
}
