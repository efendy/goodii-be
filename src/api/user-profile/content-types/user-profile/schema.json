{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "User Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "display_name": {
      "type": "string",
      "maxLength": 60,
      "required": true,
      "unique": true
    },
    "real_name": {
      "type": "string",
      "maxLength": 120
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "shops": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shop.shop",
      "mappedBy": "user_profile"
    },
    "email": {
      "type": "email"
    },
    "phone_mobile": {
      "type": "string",
      "regex": "^\\+[0-9]*"
    },
    "uid": {
      "type": "string"
    },
    "is_test": {
      "type": "boolean",
      "default": false
    },
    "geolocation": {
      "type": "component",
      "repeatable": false,
      "component": "global.geopoint"
    },
    "contacts": {
      "type": "component",
      "repeatable": true,
      "component": "global.contact-info"
    },
    "pushtokens": {
      "type": "component",
      "repeatable": true,
      "component": "global.push-tokens"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
