{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "User Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "display_name": {
      "type": "string",
      "maxLength": 60,
      "required": true,
      "unique": true
    },
    "real_name": {
      "type": "string",
      "maxLength": 120
    },
    "geohash": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "uid": {
      "type": "uid"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
