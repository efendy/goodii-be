{
  "kind": "collectionType",
  "collectionName": "user_profiles",
  "info": {
    "singularName": "user-profile",
    "pluralName": "user-profiles",
    "displayName": "User Profile",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "display_name": {
      "type": "string",
      "maxLength": 60,
      "required": true,
      "unique": true
    },
    "real_name": {
      "type": "string",
      "maxLength": 120
    },
    "geohash": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "shops": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::shop.shop",
      "mappedBy": "user_profile"
    },
    "user_kyc": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-kyc.user-kyc",
      "inversedBy": "user_profile"
    },
    "email": {
      "type": "email"
    },
    "phone_mobile": {
      "type": "string",
      "regex": "^\\+[0-9]*"
    },
    "uid": {
      "type": "string"
    }
  }
}
